        "body": [
            "\\$time = \\$_time_t + ${1:10}",
            "WHILE(\\$time > \\$_time_t)",
            "    GETSCREEN($2, $3, $4, $5)",
            "    IF_PICTURE_IN(${2:0},${3:0}, ${4:\\$_xmax},${5:\\$_ymax}, \"${6:pict}.bmp\", ${7:-1}, ${8:100})",
            "        \\$time = 0",
            "        LCLICK(\\$_return1, \\$_return2)",
            "        WAITMS(100)",
            "    ELSE",
            "        WAITMS(300)",
            "    END_IF",
            "END_CYC",
            "IF(\\$time ! 0)",
            "    LOGWRITE(\"время истекло, картинка не найдена\")",
            "END_IF",
            "$0"
        ],
        "description": "IF_PICTURE_IN ожидание картинки с таймером (шаблон)"
