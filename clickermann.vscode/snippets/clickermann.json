    "SCANPXL (шаблон) найти все пикселы": {
        "prefix": ["SCANPXL (block)", "найти все пикселы (шаблон)"],
        "body": [
            "GETSCREEN($2, $3, $4, $5)",
            "SCANPXL(\\$${1:scan}, ${2:0},${3:0}, ${4:\\$_xmax},${5:\\$_ymax}, ${6:255})",
            "\\$SHIFT_X = ${7:1}  // смещение (кол-во игнор пикселов после найденного)",
            "\\$SHIFT_Y = ${8:1}",
            "\\$scan_size = ARRSIZE(\\$scan)",
            "FOR(\\$i=0, \\$i < \\$scan_size, 2)",
            "    \\$X = \\$scan[\\$i]",
            "    \\$Y = \\$scan[\\$i + 1]",
            "    IF((\\$X > \\$X2) | (\\$Y > \\$Y2))",
            "        \\$X2 = \\$X + \\$SHIFT_X",
            "        \\$Y2 = \\$Y + \\$SHIFT_Y",
            "        ${9:MOVE(\\$X, \\$Y)}  // действие",
            "        INC(\\$scan_count, 1)",
            "        LOGWRITE(\"X: \", \\$X, \" Y: \", \\$Y)",
            "        WAITMS(100)",
            "    END_IF",
            "END_CYC",
            "UNDEFINE(\\$scan)\n",
            "LOGWRITE(\"=====================\")",
            "LOGWRITE(\"Всего найдено: \", \\$scan_count)",
            "$0"
        ],
        "description": "SCANPXL (шаблон) найти все пикселы"
    },
    "SCANPICTURE FOR (шаблон) найти все картинки": {
        "prefix": ["scanpicture (block for)", "найти все картинки (шаблон for)"],
        "body": [
            "GETSCREEN($2, $3, $4, $5)",
            "SCANPICTURE(\\$${1:scan}, ${2:0},${3:0}, ${4:\\$_xmax},${5:\\$_ymax}, \"${6:pict}.bmp\", ${7:-1}, ${8:100})",
            "",
            "\\$scan_size = ARRSIZE(\\$scan)",
            "FOR(\\$i=0, \\$i < \\$scan_size, 2)",
            "    \\$X = \\$scan[\\$i]",
            "    \\$Y = \\$scan[\\$i + 1]",
            "    ${9:MOVE(\\$X, \\$Y)}  // действие",
            "    LOGWRITE(\"X: \", \\$X, \" Y: \", \\$Y)",
            "    WAITMS(100)",
            "END_CYC",
            "UNDEFINE(\\$scan)",
            "$0"
        ],
        "description": "SCANPICTURE FOR (шаблон) найти все картинки"
    },
    "SCANPICTURE WHILE (шаблон) найти все картинки": {
        "prefix": ["scanpicture (block while)", "найти все картинки (шаблон while)"],
        "body": [
            "GETSCREEN($2, $3, $4, $5)",
            "SCANPICTURE(\\$${1:scan}, ${2:0},${3:0}, ${4:\\$_xmax},${5:\\$_ymax}, \"${6:pict}.bmp\", ${7:-1}, ${8:100})",
            "",
            "\\$ARR_NUM = 0",
            "WHILE(\\$ARR_NUM < ARRSIZE(\\$scan))",
            "    \\$X = \\$scan[\\$ARR_NUM]",
            "    \\$Y = \\$scan[\\$ARR_NUM + 1]",
            "    INC(\\$ARR_NUM, 2)",
            "    ${9:MOVE(\\$X, \\$Y)}  // действие",
            "    LOGWRITE(\"X: \", \\$X, \" Y: \", \\$Y)",
            "    WAITMS(100)",
            "END_CYC",
            "UNDEFINE(\\$scan)",
