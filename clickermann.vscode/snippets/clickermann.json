            "    IF_PICTURE_IN(${1:0},${2:0}, ${3:\\$_xmax},${4:\\$_ymax}, \"${5:pict}.bmp\", ${6:-1}, ${7:100})",
            "        \\$found = 1",
            "        LCLICK(\\$_return1, \\$_return2)",
            "        WAITMS(100)",
            "    ELSE",
            "        WAITMS(300)",
