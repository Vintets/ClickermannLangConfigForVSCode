 {
    "new": {
        "prefix": ["new", "init"],
        "body": [
            "#name \"${1:$TM_FILENAME_BASE}\"",
            "// Author: Vint",
            "// Version: 0.1 ($CURRENT_DATE.$CURRENT_MONTH.$CURRENT_YEAR)",
            "// Скрипт для Clickermann v4.13.014",
            "",
            "$0"
        ],
        "text": "Head new script",
        "description": "New default script"
    },
    "IF condition": {
        "prefix": "if",
        "body": [
            "IF(${1:\\$var = 3})",
            "    $2",
            "${3:ELSE\n    $4}",
            "END_IF",
            "$0"
        ],
        "description": "IF condition"
    },
    "FOR Loop": {
        "prefix": "for",
        "body": [
            "FOR(\\$${1|i,j,k|}=0, \\$$1 < ${2:3})",
            "    $CLIPBOARD$0",
            "END_CYC"
        ],
        "description": "FOR Loop"
    },
    "WHILE Loop": {
        "prefix": "while",
        "body": [
            "WHILE(${1:\\$var < 5})",
            "    $0",
            "END_CYC"
        ],
        "description": "WHILE Loop"
    },
}
