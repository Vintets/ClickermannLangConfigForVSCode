{
    // New
    "Head script": {
        "prefix": ["new header", "init", "header", "шапка", "автор", "заголовок"],
        "body": [
            "#name \"${1:$TM_FILENAME_BASE}\"",
            "// Author: Vint",
            "// Version: 0.1 ($CURRENT_DATE.$CURRENT_MONTH.$CURRENT_YEAR)",
            "// Скрипт для Clickermann v4.13.014",
            "",
            "$0"
        ],
        "text": "Head script",
        "description": "Head script (шапка скрипта)"
    },
    "Окно лога (шаблон)": {
        "prefix": ["log window", "окно лога"],
        "body": [
            "\\$log = 1",
            "LOGCLEAR",
            "IF(\\$log > 0)",
            "    LOGSHOW(1, \\$_xmax-335, 28)",
            "    WNDSIZE(WNDFIND(\"Лог\", 1), 336, 260)",
            "END_IF",
            "$0"
        ],
        "description": "Окно лога (шаблон)"
    },
    "Новый проект (шаблон)": {
        "prefix": ["new script", "new project", "новый скрипт", "новый проект"],
        "body": [
            "//                              Настройки",
            "//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++\n",
            "// выводить окно полного лога (0 - Не выводить, 1,2,3 - Выводить для режима)",
            "\\$log = 1\n",
            "// Браузер",
            "//\\$brawser = \"opera.exe\"",
            "//\\$brawser = \"chrome.exe\"",
            "\\$brawser = \"firefox.exe\"\n",
            "//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++\n",
            "WAITMS(500)",
            "LOGCLEAR",
            "IF(\\$log > 0)",
            "    LOGSHOW(1, \\$_xmax-335, 28)",
            "    WNDSIZE(WNDFIND(\"Лог\", 1), 336, 260)",
            "END_IF",
            "$0"
        ],
        "description": "Новый проект (шаблон)"
    },

    // condition, loop
    "IF condition": {
        "prefix": "if",
        "body": [
            "IF(${1:\\$var = 3})",
            "    $2",
            "${3:ELSE\n    $4}",
            "END_IF",
            "$0"
        ],
        "description": "IF condition"
    },
    "FOR Loop": {
        "prefix": "for",
        "body": [
            "FOR(\\$${1|i,j,k|}=0, \\$$1 < ${2:3})",
            "    $CLIPBOARD$0",
            "END_CYC"
        ],
        "description": "FOR Loop"
    },
    "WHILE Loop": {
        "prefix": "while",
        "body": [
            "WHILE(${1:\\$var < 5})",
            "    $0",
            "END_CYC"
        ],
        "description": "WHILE Loop"
    },
    "SWITCH condition": {
        "prefix": "switch",
        "body": [
            "SWITCH(${1:\\$var})",
            "    CASE(1)",
            "        $2",
            "    CASE(2)",
            "        $3",
            "${4:    DEFAULT\n        $5}",
            "END_SWITCH",
            "$0"
        ],
        "description": "SWITCH condition"
    },
    "Is key down": {
        "prefix": "iskeydown",
        "body": [
            "IF(ISKEYDOWN(${1:#SPACE}) = ${2:1})",
            "    $0",
            "END_IF"
        ],
        "description": "Is key down"
    },
    "Условие с таймером, секунды": {
        "prefix": ["timer sec", "таймер сек"],
        "body": [
            "IF(\\$time < \\$_time_t)",
            "    $0",
            "    \\$time = \\$_time_t + ${1:60}  // условие сработает снова через $1 сек",
            "END_IF"
        ],
        "description": "Условие с таймером, секунды"
    },
    "Условие с таймером, миллисекунды": {
        "prefix": ["timer ms", "таймер мс"],
        "body": [
            "IF(\\$time < \\$_ms)",
            "    $0",
            "    \\$time = \\$_ms + ${1:60000}  // условие сработает снова через $1 мс",
            "END_IF"
        ],
        "description": "Условие с таймером, миллисекунды"
    },

