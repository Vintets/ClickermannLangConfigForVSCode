{
    // New
    "Head script": {
        "prefix": ["new header", "init", "header", "шапка", "автор", "заголовок"],
        "body": [
            "#name \"${1:$TM_FILENAME_BASE}\"",
            "// Author: Vint",
            "// Version: 0.1 ($CURRENT_DATE.$CURRENT_MONTH.$CURRENT_YEAR)",
            "// Скрипт для Clickermann v4.13.014",
            "",
            "$0"
        ],
        "text": "Head script",
        "description": "Head script (шапка скрипта)"
    },
    "Окно лога (шаблон)": {
        "prefix": ["log window", "окно лога"],
        "body": [
            "\\$log = 1",
            "LOGCLEAR",
            "IF(\\$log > 0)",
            "    LOGSHOW(1, \\$_xmax-335, 28)",
            "    WNDSIZE(WNDFIND(\"Лог\", 1), 336, 260)",
            "END_IF",
            "$0"
        ],
        "description": "Окно лога (шаблон)"
    },
    "Новый проект (шаблон)": {
        "prefix": ["new script", "new project", "новый скрипт", "новый проект"],
        "body": [
            "//                              Настройки",
            "//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++\n",
            "// выводить окно полного лога (0 - Не выводить, 1,2,3 - Выводить для режима)",
            "\\$log = 1\n",
            "// Браузер",
            "//\\$brawser = \"opera.exe\"",
            "//\\$brawser = \"chrome.exe\"",
            "\\$brawser = \"firefox.exe\"\n",
            "//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++\n",
            "WAITMS(500)",
            "LOGCLEAR",
            "IF(\\$log > 0)",
            "    LOGSHOW(1, \\$_xmax-335, 28)",
            "    WNDSIZE(WNDFIND(\"Лог\", 1), 336, 260)",
            "END_IF",
            "$0"
        ],
        "description": "Новый проект (шаблон)"
    },

    // condition, loop
    "IF condition": {
        "prefix": "if",
        "body": [
            "IF(${1:\\$var = 3})",
            "    $2",
            "${3:ELSE\n    $4}",
            "END_IF",
            "$0"
        ],
        "description": "IF condition"
    },
    "FOR Loop": {
        "prefix": "for",
        "body": [
            "FOR(\\$${1|i,j,k|}=0, \\$$1 < ${2:3})",
            "    $CLIPBOARD$0",
            "END_CYC"
        ],
        "description": "FOR Loop"
    },
    "WHILE Loop": {
        "prefix": "while",
        "body": [
            "WHILE(${1:\\$var < 5})",
            "    $0",
            "END_CYC"
        ],
        "description": "WHILE Loop"
    },
    "SWITCH condition": {
        "prefix": "switch",
        "body": [
            "SWITCH(${1:\\$var})",
            "    CASE(1)",
            "        $2",
            "    CASE(2)",
            "        $3",
            "${4:    DEFAULT\n        $5}",
            "END_SWITCH",
            "$0"
        ],
        "description": "SWITCH condition"
    },
    "Is key down": {
        "prefix": "iskeydown",
        "body": [
            "IF(ISKEYDOWN(${1:#SPACE}) = ${2:1})",
            "    $0",
            "END_IF"
        ],
        "description": "Is key down"
    },
    "Условие с таймером, секунды": {
        "prefix": ["timer sec", "таймер сек"],
        "body": [
            "IF(\\$time < \\$_time_t)",
            "    $0",
            "    \\$time = \\$_time_t + ${1:60}  // условие сработает снова через $1 сек",
            "END_IF"
        ],
        "description": "Условие с таймером, секунды"
    },
    "Условие с таймером, миллисекунды": {
        "prefix": ["timer ms", "таймер мс"],
        "body": [
            "IF(\\$time < \\$_ms)",
            "    $0",
            "    \\$time = \\$_ms + ${1:60000}  // условие сработает снова через $1 мс",
            "END_IF"
        ],
        "description": "Условие с таймером, миллисекунды"
    },

    // subroutines, threads
    "SUB subroutines (подпрограмма)": {
        "prefix": ["sub", "подпрограмма"],
        "body": [
            "SUB(${1:name}, ${2:\\$par1, \\$par2})",
            "    $0",
            "END_SUB"
        ],
        "description": "SUB subroutines (подпрограмма)"
    },
    "THREAD (поток)": {
        "prefix": ["thread", "поток"],
        "body": [
            "THREAD(${1:name}, ${2:1})",
            "    $0",
            "END_THREAD"
        ],
        "description": "THREAD (поток)"
    },
    "SETTHREAD (управление потоком)": {
        "prefix": ["setthread", "управление потоком"],
        "body": [
            "SETTHREAD(${1:thr_name}, ${2:1})  // 0-пауза, 1-старт, 2-стоп",
            "$0"
        ],
        "description": "SETTHREAD (управление потоком)"
    },

    // Log
    "#logfile": {
        "prefix": ["logfile", "лог файл"],
        "body": [
            "#logfile \"${1:debug.txt}\"",
            "$0"
        ],
        "description": "#logfile"
    },
    "LOGSHOW": {
        "prefix": ["logshow", "лог показать"],
        "body": [
            "LOGSHOW(1, ${1:100},${2:100})",
            "$0"
        ],
        "description": "LOGSHOW"
    },
    "LOGCLEAR": {
        "prefix": ["logclear", "лог очистить"],
        "body": [
            "LOGCLEAR",
            "$0"
        ],
        "description": "LOGCLEAR"
    },
    "LOGWRITE": {
        "prefix": ["logwrite", "print", "вывод в лог"],
        "body": [
            "LOGWRITE(\"$1\")",
            "$0"
        ],
        "description": "LOGWRITE"
    },
    "Log1 (шаблон)": {
        "prefix": ["logwrite", "print", "вывод в лог", "log1"],
        "body": [
            "LOGWRITE(\"X/Y  \", \\$_return1, \" / \", \\$_return2)",
            "$0"
        ],
        "description": "Log1 (шаблон)"
    },
    "Log2 (шаблон)": {
        "prefix": ["logwrite", "print", "вывод в лог", "log2"],
        "body": [
            "LOGWRITE(\"X=\", \\$_return1, \" Y=\", \\$_return2)",
            "$0"
        ],
        "description": "Log2 (шаблон)"
    },
    "Log massive (шаблон)": {
        "prefix": ["logwrite massive", "print massive", "лог массива"],
        "body": [
            "FOR(\\$i=0, \\$i < ARRSIZE(\\$${1:arr}))",
            "    LOGWRITE(\"[\", \\$i, \"]  \", \\$$1[\\$i])",
            "END_CYC",
            "$0"
        ],
        "description": "Log massive (шаблон)"
    },
    "HINTPOPUP": {
        "prefix": ["hintpopup", "traysay", "сообщение в трей", "tray"],
        "body": [
            "HINTPOPUP(\"${1:Сообщение}\", \"${2:Заголовок}\")",
            "$0"
        ],
        "description": "HINTPOPUP"
    },
    "SOUND play": {
        "prefix": ["sound", "play", "звуковой файл"],
        "body": [
            "SOUND(\"${1:play}.wav\")",
            "$0"
        ],
        "description": "SOUND play"
    },
    "BEEP note": {
        "prefix": ["beep", "бипер", "звуковой сигнал"],
        "body": [
            "BEEP(${1:800}, ${2:200})",
            "$0"
        ],
        "description": "SOUND play"
    },

    // Preprocessor
    "#name": {
        "prefix": ["name", "имя скрипта", "название"],
        "body": [
            "#name \"${1:СуперБот v1.2}\"",
            "$0"
        ],
        "description": "#name"
    },
    "#include": {
        "prefix": ["include", "подключить скрипт"],
        "body": [
            "#include \"${1:mylib}.cms\"",
            "$0"
        ],
        "description": "#include"
    },
    "#autorun": {
        "prefix": ["autorun", "автозапуск"],
        "body": [
            "#autorun",
            "$0"
        ],
        "description": "#autorun"
    },
    "#define": {
        "prefix": ["define", "макрос"],
        "body": [
            "#define ${1:find}:${2:replace}",
            "$0"
        ],
        "description": "#define"
    },
    "#ps2 keyboard": {
        "prefix": ["ps2_keyboard", "ps/2 клавиатура"],
        "body": [
            "#ps2_keyboard",
            "$0"
        ],
        "description": "#ps2 keyboard"
    },
    "#ps2 mouse": {
        "prefix": ["ps2_mouse", "ps/2 мышь"],
        "body": [
            "#ps2_mouse",
            "$0"
        ],
        "description": "#ps2 mouse"
    },

    // Keyboard
    "KEYPRESS Нажать клавишу": {
        "prefix": ["keypress", "нажать клавишу"],
        "body": [
            "KEYPRESS(#${1:SPACE})",
            "$0"
        ],
        "description": "KEYPRESS Нажать клавишу"
    },
    "KEYDOWN Зажать клавишу": {
        "prefix": ["keydown", "зажать клавишу"],
        "body": [
            "KEYDOWN(#${1:SPACE})",
            "$0"
        ],
        "description": "KEYDOWN Зажать клавишу"
    },
    "KEYUP Отпустить клавишу": {
        "prefix": ["keyup", "отпустить клавишу"],
        "body": [
            "KEYUP(#${1:SPACE})",
            "$0"
        ],
        "description": "KEYUP Отпустить клавишу"
    },
    "KEYSTRING Набрать текст": {
        "prefix": ["keystring", "набрать текст"],
        "body": [
            "KEYSTRING(\"${1:str}\", ${2:50})",
            "$0"
        ],
        "description": "KEYSTRING Набрать текст"
    },
    "GETKEYSDOWN Поместить в массив зажатые клавиши": {
        "prefix": "getkeysdown",
        "body": [
            "GETKEYSDOWN(\\$${1:arr})",
            "$0"
        ],
        "description": "GETKEYSDOWN Поместить в массив зажатые клавиши"
    },
    "GETKBLAYOUT Получить код раскладки клавиатуры окна": {
        "prefix": ["getkblayout", "получить язык"],
        "body": [
            "GETKBLAYOUT(\\$${1:hwnd})  //1033 En 1049 Ru",
            "$0"
        ],
        "description": "GETKBLAYOUT Получить код раскладки клавиатуры окна"
    },
    "SETKBLAYOUT Установить код раскладки клавиатуры в окне": {
        "prefix": ["setkblayout", "установить язык"],
        "body": [
            "SETKBLAYOUT(\\$${1:hwnd}, ${2:1033})  //1033 En 1049 Ru",
            "$0"
        ],
        "description": "SETKBLAYOUT Установить код раскладки клавиатуры в окне"
    },
    "Ctrl+V (шаблон)": {
        "prefix": ["ctrl+v", "комбинация клавиш"],
        "body": [
            "KEYDOWN(#${1:CTRL})",
            "WAITMS(30)",
            "KEYPRESS(#${2:V})",
            "WAITMS(30)",
            "KEYUP(#$1)",
            "WAITMS(100)",
            "$0"
        ],
        "description": "Ctrl+V (шаблон)"
    },
    "KeyDown + KeyUp (шаблон)": {
        "prefix": ["keydown+keyup", "нажать+отпустить клавишу"],
        "body": [
            "KEYDOWN(#${1:A})",
            "WAITMS(30)",
            "KEYUP(#$1)",
            "WAITMS(50)",
            "$0"
        ],
        "description": "KeyDown + KeyUp (шаблон)"
    },

    // Mouse
    "LCLICK Левый клик": {
